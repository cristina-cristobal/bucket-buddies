
<%= g.fields_for :likes, @goal.likes.build do |l| %>
  <%#= l.label :description %>
  <%= l.text_area :description, placeholder: 'description', class: 'form-field' %><br>
  <%= l.date_select(:timeline, :order => [:month, :day, :year], prompt: true) %><br>
  <%= l.label :timeline, "Complete By" %>
  <%= l.hidden_field :user_id, value: session[:user_id] %>
<% end %>
